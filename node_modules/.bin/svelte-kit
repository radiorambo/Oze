#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/zero/Code/kit/packages/kit/node_modules:/home/zero/Code/kit/packages/node_modules:/home/zero/Code/kit/node_modules:/home/zero/Code/node_modules:/home/zero/node_modules:/home/node_modules:/node_modules:/home/zero/Code/kit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/zero/Code/kit/packages/kit/node_modules:/home/zero/Code/kit/packages/node_modules:/home/zero/Code/kit/node_modules:/home/zero/Code/node_modules:/home/zero/node_modules:/home/node_modules:/node_modules:/home/zero/Code/kit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../packages/kit/svelte-kit.js" "$@"
else
  exec node  "$basedir/../../../../packages/kit/svelte-kit.js" "$@"
fi
