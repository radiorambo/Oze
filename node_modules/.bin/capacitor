#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/zero/Code/kit/node_modules/.pnpm/@capacitor+cli@6.0.0/node_modules/@capacitor/cli/bin/node_modules:/home/zero/Code/kit/node_modules/.pnpm/@capacitor+cli@6.0.0/node_modules/@capacitor/cli/node_modules:/home/zero/Code/kit/node_modules/.pnpm/@capacitor+cli@6.0.0/node_modules/@capacitor/node_modules:/home/zero/Code/kit/node_modules/.pnpm/@capacitor+cli@6.0.0/node_modules:/home/zero/Code/kit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/zero/Code/kit/node_modules/.pnpm/@capacitor+cli@6.0.0/node_modules/@capacitor/cli/bin/node_modules:/home/zero/Code/kit/node_modules/.pnpm/@capacitor+cli@6.0.0/node_modules/@capacitor/cli/node_modules:/home/zero/Code/kit/node_modules/.pnpm/@capacitor+cli@6.0.0/node_modules/@capacitor/node_modules:/home/zero/Code/kit/node_modules/.pnpm/@capacitor+cli@6.0.0/node_modules:/home/zero/Code/kit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/@capacitor+cli@6.0.0/node_modules/@capacitor/cli/bin/capacitor" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/@capacitor+cli@6.0.0/node_modules/@capacitor/cli/bin/capacitor" "$@"
fi
